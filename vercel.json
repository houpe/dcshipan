{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "VERCEL_ENV": "1",
    "PYTHONPATH": "."
  },
  "functions": {
    "index.py": {
      "maxDuration": 10
    }
  }
}